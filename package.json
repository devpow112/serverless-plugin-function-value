{
  "name": "serverless-plugin-function-value",
  "version": "1.2.24",
  "description": "Serverless framework plugin that will automatically generate CloudFormation snippets to reference a functions name, ARN or logical ID value based on the generated logical ID used during creation of the CloudFormation templates",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel --out-dir dist src",
    "lint": "npm run -s lint:git && npm run -s lint:js && npm run -s lint:md",
    "lint:js": "eslint .",
    "lint:md": "markdownlint .",
    "lint:git": "commitlint --from origin/main --to HEAD",
    "format": "npm run -s format:js && npm run -s format:md",
    "format:js": "npm run -s lint:js -- --fix",
    "format:md": "npm run -s lint:md -- --fix",
    "test": "npm run -s lint && npm run -s test:unit",
    "test:unit": "nyc mocha --colors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devpow112/serverless-plugin-function-value.git"
  },
  "keywords": [
    "aws",
    "cloudformation",
    "serverless",
    "plugin"
  ],
  "author": "Devon Powell <devon.f.powell@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devpow112/serverless-plugin-function-value/issues"
  },
  "homepage": "https://github.com/devpow112/serverless-plugin-function-value#readme",
  "files": [
    "dist/**/*.js"
  ],
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "lodash.memoize": "^4.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/register": "^7.13.8",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@semantic-release/git": "^9.0.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "chai": "^4.3.3",
    "conventional-changelog-conventionalcommits": "^4.5.0",
    "eslint": "^7.21.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha": "^8.0.0",
    "markdownlint-cli": "^0.27.1",
    "mocha": "^8.3.1",
    "nyc": "^15.1.0",
    "semantic-release": "^17.4.1",
    "sinon": "^9.2.4"
  }
}
