{
  "name": "serverless-plugin-function-value",
  "version": "1.0.6",
  "description": "Serverless framework plugin that will automatically generate CloudFormation snippets to reference a functions name or arn value based on the generated logical ID used during creation of the CloudFormation templates",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel --out-dir dist src",
    "lint": "eslint .",
    "format": "npm run lint -s -- --fix",
    "test": "npm run lint -s && nyc mocha --colors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devpow112/serverless-plugin-function-value.git"
  },
  "keywords": [
    "aws",
    "cloudformation",
    "serverless",
    "plugin"
  ],
  "author": "Devon Powell <devon.f.powell@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devpow112/serverless-plugin-function-value/issues"
  },
  "homepage": "https://github.com/devpow112/serverless-plugin-function-value#readme",
  "files": [
    "dist/**/*.js"
  ],
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@semantic-release/git": "^9.0.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "chai": "^4.2.0",
    "eslint": "^7.8.1",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-mocha": "^8.0.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "semantic-release": "^17.1.1",
    "sinon": "^9.0.3"
  }
}
